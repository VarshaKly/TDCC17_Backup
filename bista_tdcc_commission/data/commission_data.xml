<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences: Sales Commission -->
        <record id="sequence_sales_commission_seq" model="ir.sequence">
            <field name="name">Sales Commission Sequence</field>
            <field name="code">sale.commission</field>
            <field name="prefix">SC</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
        </record>

        <!-- Journal: Sale Commission -->
        <record model="account.journal" id="tdcc_commission_journal">
            <field name="name">Commission - TDCC</field>
            <field name="code">COM</field>
            <field name="type">general</field>
        </record>


    </data>
</odoo>

<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Added DHA and CDA license -->
    <record model="ir.ui.view" id="view_company_form_tdcc">
        <field name="name">view.company.form.tdcc</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <xpath expr="//page//div" position="after">
                <field name="dha_license_no" />
                <field name="cda_license_no" />
                <field name="day_closing_user_id" />
                <field name="day_closing_date" />
            </xpath>
        </field>
    </record>

</odoo>
